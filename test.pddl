(define (domain gripper)
  (:predicates (room ?r) (ball ?b) (gripper ?g)
               (at-robby ?r) (at ?b ?r)
               (free ?g) (carry ?o ?g))
  (:action move
     :parameters (?from ?to)
     :precondition (and (room ?from)
                        (room ?to)
                        (at-robby ?from))
     :effect (and (at-robby ?to)
                  (not (at-robby ?from)))))
